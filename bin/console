use Kuick\App\Events\CommandReceivedEvent;
use Kuick\App\Kernel;

$projectDir = dirname(__DIR__);
require $projectDir . '/vendor/autoload.php';

$kernel = new Kernel($projectDir);
$kernel->getEventDispatcher()->dispatch(new CommandReceivedEvent($kernel));
